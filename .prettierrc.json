{
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "overrides": [{
    "files": [
      "*.html"
    ],
    "options": {
      "htmlWhitespaceSensitivity": "ignore",
      "jsxBracketSameLine": true
    }
  }],
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 4,
  "trailingComma": "es5",
  "useTabs": false
}
